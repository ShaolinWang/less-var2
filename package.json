{
  "publisher": "shaolinwang",
  "name": "less-var2",
  "displayName": "less-var2",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.0.1",
  "description": "chore version of less-vars",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaolinWang/less-vars.git"
  },
  "main": "./dist/index.js",
  "contributes": {
    "configuration": {
      "properties": {
        "lessVars.locations": {}
      }
    },
    "commands": [
      {
        "command": "setLocations",
        "title": "设置less变量文件路径"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == less",
          "command": "setLocations",
          "group": "navigation"
        }
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.68.0"
  },
  "activationEvents": [
    "onLanguage:less"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@babel/types": "^7.18.4",
    "@types/node": "^17.0.41",
    "@types/vscode": "^1.68.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "pnpm": "^7.1.9",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.11",
    "vitest": "^0.14.1",
    "vsce": "^2.9.1"
  },
  "dependencies": {
    "color-alpha": "^1.1.3",
    "color-rgba": "^2.2.3",
    "get-css-colors": "^4.0.0",
    "less-vars-to-js": "^1.3.0"
  }
}
